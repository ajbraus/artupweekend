<div id="contact_us" class="row contact_us">
	<div class="clearfix block">
		<h1 class="text-center">Contact us</h1>
		<%= form_for @email_subscription do |f| %>
		  <% if @email_subscription.errors.any? %>
		    <div id="error_explanation">
		      <h2><%= pluralize(@email_subscription.errors.count, "error") %> prohibited this email_subscription from being saved:</h2>

		      <ul>
		      <% @email_subscription.errors.full_messages.each do |msg| %>
		        <li><%= msg %></li>
		      <% end %>
		      </ul>
		    </div>
		  <% end %>

		  <%= f.label :name, "name*" %>
			<%= f.text_field :name %>

		  <%= f.label :email, "email*" %>
			<%= f.text_field :email %>

		  <%= f.label :phone, "phone (optional)" %>
			<%= f.text_field :phone %>

		  <%= f.label :message, "message*" %>
			<%= f.text_area :message %>

			<br>
			<%= f.check_box :newsletter, class:'pull-left', checked: true %>
			<%= f.label :newsletter, "Recieve emails from us every now and then" %>

			<%= f.submit "Contact Us" %>
		<% end %>
